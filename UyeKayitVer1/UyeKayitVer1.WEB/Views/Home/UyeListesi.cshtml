
@model List<UyeKayitVer1.Entitiy.Viewmodel.PersonVM>
@{
    ViewBag.Title = "Üye Listesi";
    }
<h2>Üye Listesi</h2>

<hr style="border-color:crimson" />
<div class="panel-body">
    <div class="row">
        <div class="col-lg-12">
            <div class="table-responsive">
                <table class="table table-bordered table-hover table-striped">
                    <thead>
                        <tr>
                            <th>İsim</th>
                            <th>Soy Adı</th>
                            <th>Email</th>
                            <th>Telefon</th>
                            <th>Üyelik Sınıfı</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            @{
                                foreach (var item in Model)
                                {
                                    <td>@item.Pname</td>
                                    <td>@item.Lastname</td>
                                    <td>@item.Email</td>
                                    <td>@item.Phone</td>
                                    <td>@item.TName</td>
                                }
                            }
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-lg-8">
            <div id="morris-bar-chart"></div>
        </div>
    </div>
</div>
<hr style="border-color:crimson" />
